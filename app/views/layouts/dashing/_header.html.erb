<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Dish Dash</title>
  <!-- Bootstrap -->
  <link href="assets/css/bootstrap.css" rel="stylesheet">
  <link href="assets/css/style.css" rel="stylesheet">
  <link href="assets/css/override-bootstrap.css" rel="stylesheet">
  <link href="assets/css/font-awesome.css" rel="stylesheet">
</head>

<div class="container">
  <div class="navbar-header">
    <button data-target=".bs-navbar-collapse" data-toggle="collapse" type="button" class="navbar-toggle collapsed">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <div id="logo">
      <a class="navbar-brand" > </a>
    </div>

  </div>
  <nav class="collapse navbar-collapse bs-navbar-collapse">
    <ul class="nav navbar-nav">
      <!-- navbar lists -->
      <li>
        <%= link_to  admin_dashboards_path,:class=>"border-left border-right" do %>
          <i class="fa fa-tachometer header-icons"></i><br />
          manage dashboard
        <% end %>
      </li>
      <li>
        <%= link_to  admin_users_path, :class =>"border-right" do %>
        <i class="fa fa-cogs header-icons"></i> <br />
        manage users
        <% end %>
      </li>
    </ul>
    <!-- user dropdown -->
    <ul class="nav navbar-nav navbar-right">
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
          <i class="fa fa-user header-icons"></i> <br />
          <%= current_user.name %> <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><a href="#">Action</a></li>
            <li><a href="#">Another action</a></li>
            <li class="divider"></li>
            <li><%= link_to "Sign Out", admin_session_path(current_user), method: :delete %></li></li>
          </ul>
        </li>
      </ul>
  </div>


 